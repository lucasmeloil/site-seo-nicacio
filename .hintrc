{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-tap-highlight-color",
          "-webkit-overflow-scrolling",
          "min-height: auto",
          "-ms-touch-action"
        ]
      }
    ]
  }
}